package coding.ninja;

import java.util.Scanner;

public class Test {			
	
	public static void main(String[] args) {
		
		int [] arr = {4,3,1,2,9,6,7,11,10,0,-1};
		
		
		int min = arr[0];
		for(int i=1;i<arr.length;i++) {
			
			if(arr[i]<min) {
				
				min=arr[i];
				
			}
			
		}
	     System.out.println("Minimun in the given array is "+ min);  
	     
 }  
		
}	
