package coding.ninja;

import java.util.Scanner;

public class Test {			
	
  // Maximum in an array
	public static void main(String[] args) {
		
		int [] arr = {4,3,1,2,9,6,7,11,10,0,-1};
		
		
		int max = arr[0];
		for(int i=1;i<arr.length;i++) {
			
			if(arr[i]>max) {
				
				max=arr[i];
				
			}
			
		}
	     System.out.println("Maximun from the given array is "+ max);  
	     
 }  
		
}	
